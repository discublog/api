{
  "name": "@discublog/api",
  "version": "1.0.0",
  "description": "A ready-to-use, reusable wrapper for the GitHub Discussions API, based on the GitHub GraphQL API.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./interface": {
      "types": "./dist/interface.d.ts"
    }
  },
  "keywords": [
    "GitHub API",
    "GitHub Discussions",
    "GitHub Discussions API",
    "GitHub GraphQL API"
  ],
  "author": "zhangyu1818 <hey@zhangyu.dev>",
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "format": "npx prettier --write ."
  },
  "license": "MIT",
  "dependencies": {
    "@octokit/core": "^6.0.1"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  },
  "files": [
    "dist"
  ]
}
